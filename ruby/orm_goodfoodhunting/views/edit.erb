<h1>edit dish</h1>
<form action="/dishes/<%= @id %>" method = "post">
	<input type="hidden" name="_method" value="put">
	<input type="hidden" name="id" value=<%= @id %>>
	name:<input type="text" name="name" value=<%= @dish.name %>>
	image_url:<input type="text" name="image_url" value=<%= @dish.image_url %>>
	dish type:<input type="text" name="dish_type" value=<%= @dish.dish_type_id %>>
	<button>submit</button>
</form>